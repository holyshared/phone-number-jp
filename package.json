{
  "name": "tel-splitter-jp",
  "version": "0.1.0",
  "description": "The phone number splitter for Japanese",
  "main": "lib/index.js",
  "files": [
    "lib/*.js",
    "README.md"
  ],
  "scripts": {
    "test": "multi='dot=- mocha-istanbul=-' ISTANBUL_REPORTERS='text-summary,lcovonly' ./node_modules/.bin/mocha test",
    "instrument": "node_modules/.bin/istanbul instrument lib -o lib-cov",
    "build": "./node_modules/.bin/babel -d lib -s inline src",
    "watch": "./node_modules/.bin/babel -d lib -s inline -w src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holyshared/tel-splitter-jp.git"
  },
  "author": "Noritaka Horio <holy.shared.design@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/tel-splitter-jp/issues"
  },
  "homepage": "https://github.com/holyshared/tel-splitter-jp#readme",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "codecov": "^1.0.1",
    "coffee-script": "^1.10.0",
    "eslint": "^1.8.0",
    "espower-coffee": "^1.0.0",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mocha-istanbul": "^0.2.0",
    "mocha-multi": "^0.7.2",
    "mysql": "^2.9.0",
    "power-assert": "^1.1.0"
  }
}
